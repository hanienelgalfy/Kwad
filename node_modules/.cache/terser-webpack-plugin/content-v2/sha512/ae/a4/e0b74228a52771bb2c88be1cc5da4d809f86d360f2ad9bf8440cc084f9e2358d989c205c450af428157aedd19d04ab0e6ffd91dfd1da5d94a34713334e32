\\" className={classes.text}>\\n            {editing ? this.renderTitleInput(task) : renderTitle(task)}\\n          </Typography>\\n\\n          <ListItemSecondaryAction>\\n            <Hidden xsDown>\\n              <IconButton\\n                className={\\n                  classNames(\\n                    classes.button,\\n                    editing && classes.hide\\n                  )\\n                }\\n                size=\\\"small\\\"\\n                onClick={this.edit}\\n              >\\n                <EditIcon />\\n              </IconButton>\\n              <IconButton\\n                className={\\n                  classNames(\\n                    classes.button,\\n                    editing && classes.hide\\n                  )\\n                }\\n                size=\\\"small\\\"\\n                onClick={this.remove}\\n              >\\n                <DeleteIcon />\\n              </IconButton>\\n            </Hidden>\\n            <Hidden smUp>\\n              <IconButton\\n                aria-label=\\\"More\\\"\\n                aria-owns={open ? 'long-menu' : undefined}\\n                aria-haspopup=\\\"true\\\"\\n                onClick={this.handleClick}\\n                className={editing ? classes.hide : ''}\\n              >\\n                <MoreVertIcon />\\n              </IconButton>\\n              <Menu\\n                id=\\\"long-menu\\\"\\n                anchorEl={anchorEl}\\n                open={open}\\n                onClose={this.handleClose}\\n              >\\n                <MenuItem onClick={this.edit}>\\n                  Edit\\n                </MenuItem>\\n                <MenuItem onClick={this.remove}>\\n                  Remove\\n                </MenuItem>\\n              </Menu>\\n            </Hidden>\\n            <IconButton\\n              className={\\n                classNames(\\n                  classes.button,\\n                  !editing && classes.hide\\n                )\\n              }\\n              size=\\\"small\\\"\\n              onClick={this.stopEditing}\\n            >\\n              <CancelIcon />\\n            </IconButton>\\n          </ListItemSecondaryAction>\\n        </ListItem>\\n        <Divider />\\n      </Fragment>\\n    );\\n  }\\n}\\n\\nTaskItem.propTypes = {\\n  classes: PropTypes.object.isRequired,\\n  removeTask: PropTypes.func.isRequired,\\n  task: PropTypes.object.isRequired,\\n  updateTask: PropTypes.func.isRequired\\n};\\n\\n\\nexport default withStyles(styles)(TaskItem);\\n\",\"import React, { Fragment } from 'react';\\nimport PropTypes from 'prop-types';\\nimport { withStyles } from '@material-ui/core/styles';\\nimport classNames from 'classnames';\\nimport List from '@material-ui/core/List';\\nimport ListItem from '@material-ui/core/ListItem';\\nimport Divider from '@material-ui/core/Divider';\\nimport styles from './todo-jss';\\n\\nclass PlaceLoader extends React.Component {\\n  render() {\\n    const { loop, classes } = this.props;\\n    const renderElm = [...Array(loop)].map((e, i) => (\\n      <Fragment key={i.toString()}>\\n        <ListItem\\n          role={undefined}\\n          dense\\n          className={classes.listItem}\\n        >\\n          <div className={classes.placeLoader}>\\n            <span className={classNames(classes.img, classes.button)} />\\n            <span className={classes.title} />\\n          </div>\\n        </ListItem>\\n        <Divider />\\n      </Fragment>\\n    ));\\n    return (\\n      <List className={classes.taskList}>\\n        {renderElm}\\n      </List>\\n    );\\n  }\\n}\\n\\nPlaceLoader.propTypes = {\\n  classes: PropTypes.object.isRequired,\\n  loop: PropTypes.number.isRequired,\\n};\\n\\nexport default withStyles(styles)(PlaceLoader);\\n\",\"import React from 'react';\\nimport { List } from 'immutable';\\nimport { withStyles } from '@material-ui/core/styles';\\nimport PropTypes from 'prop-types';\\nimport ListWrap from '@material-ui/core/List';\\ni